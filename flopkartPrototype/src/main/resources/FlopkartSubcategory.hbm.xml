<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="com.iiitb.ooadvoid.pojo.FlopkartSubcategory" table="flopkartSubcategory"  dynamic-update="true">
      
      <id name = "id" type = "integer" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "categoryId" column = "categoryId" type = "integer"  unique="true"/>
      <property name = "subcategoryName" column = "subcategoryName" type = "string"/>
	 
	</class>
</hibernate-mapping>